<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="language.Language" table="languages">
		<id name="id" type="int">
			<column name="LANG_ID" precision="38" scale="0" />
			<generator class="assigned" />
		</id>
		<property name="name" type="string">
			<column name="LANG_NAME" length="30" />
		</property>
		<property name="isoCode" type="string">
			<column name="LANG_ISO" length="5" />
		</property>
	</class>
</hibernate-mapping>
